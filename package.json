{
  "name": "scraper-service",
  "version": "1.0.0",
  "description": "Production-grade scraping infrastructure with NestJS, Playwright, and Fetch",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "setup": "npm install && npx prisma generate",
    "build": "nest build",
    "build:api": "nest build api",
    "build:worker": "nest build worker",
    "build:browser": "nest build browser-service",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bash scripts/start.sh",
    "start:prod": "bash scripts/start.sh --prod -d",
    "stop": "bash scripts/start.sh --down",
    "stop:prod": "bash scripts/start.sh --prod --down",
    "logs": "bash scripts/start.sh --logs",
    "logs:prod": "bash scripts/start.sh --prod --logs",
    "ps": "bash scripts/start.sh --ps",
    "ps:prod": "bash scripts/start.sh --prod --ps",
    "restart:prod": "npm run stop:prod && npm run start:prod",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:worker\"",
    "dev:api": "nest start api --watch",
    "dev:worker": "nest start worker --watch",
    "dev:browser": "nest start browser-service --watch",
    "prod:api": "node dist/src/apps/api/main",
    "prod:worker": "node dist/src/apps/worker/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:push": "prisma db push"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/multipart": "^9.4.0",
    "@google/genai": "^1.39.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/extension-read-replicas": "^0.5.0",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.67.2",
    "cache-manager": "^6.1.0",
    "cache-manager-redis-yet": "^5.1.5",
    "chalk": "^4.1.2",
    "cheerio": "^1.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "keyv": "^5.1.0",
    "nodemailer": "^8.0.0",
    "paapi5-nodejs-sdk": "^1.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.17.2",
    "playwright": "^1.40.1",
    "playwright-chromium": "^1.40.1",
    "prisma": "^7.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "undici": "^6.2.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.7",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
