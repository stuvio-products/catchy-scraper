{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false
  },
  "projects": {
    "api": {
      "type": "application",
      "root": ".",
      "entryFile": "apps/api/main",
      "sourceRoot": ".",
      "compilerOptions": {
        "tsConfigPath": "src/apps/api/tsconfig.app.json"
      }
    },
    "worker": {
      "type": "application",
      "root": ".",
      "entryFile": "apps/worker/main",
      "sourceRoot": ".",
      "compilerOptions": {
        "tsConfigPath": "src/apps/worker/tsconfig.app.json"
      }
    },
    "browser-service": {
      "type": "application",
      "root": "src/apps/browser-service",
      "entryFile": "main",
      "sourceRoot": "src/apps/browser-service",
      "compilerOptions": {
        "tsConfigPath": "src/apps/browser-service/tsconfig.app.json"
      }
    }
  }
}
