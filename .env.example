# Shared Configuration
NODE_ENV=development
REDIS_HOST=redis
REDIS_PORT=6379

# Scraper API Configuration
API_PORT=3000
API_KEY=dev-api-key-change-in-production

# Backend Configuration
DATABASE_URL="postgresql://postgres:postgres123@db:5432/catchy_db?schema=public"
DATABASE_REPLICA_URL="postgresql://postgres:postgres123@db:5432/catchy_db?schema=public"
JWT_SECRET="your_jwt_secret"
SECRET_SALT="your_secret_salt"
GEMINI_API_KEY="your_gemini_api_key"

# Email Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_app_password

# Worker Configuration
WORKER_CONCURRENCY=4

# Browser Service Configuration
BROWSER_SERVICE_HOST=browser-service
BROWSER_SERVICE_PORT=3001
BROWSER_SERVICE_API_KEY=internal-browser-service-key
BROWSER_COUNT=2
BROWSER_HEADLESS=true
BROWSER_TIMEOUT_MS=30000
# DEBUG_BROWSER_DOMAINS=meesho.com (comma-separated list of domains to show browser for debugging)

# Proxy Provider Configuration
PROXY_PROVIDER=fake
FAKE_PROXY_COST_PER_MB=0.001
FAKE_PROXY_FAILURE_RATE=0.05
